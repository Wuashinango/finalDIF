<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:screenOrientation="portrait"
        android:supportsRtl="true"
        android:theme="@style/Theme.AplicacionFinal"
        tools:targetApi="31">
        <activity
            android:name=".ActivityInventario"
            android:exported="false" />
        <activity
            android:name=".ActivityReport"
            android:exported="false" />
        <activity
            android:name=".ActivityAyudantes"
            android:exported="false" />
        <activity
            android:name=".ActivityScannerAyudantes"
            android:exported="false"
            android:theme="@style/Theme.AplicacionFinal" />
        <activity
            android:name=".ActivityScannerCorrecto"
            android:exported="false" />
        <activity
            android:name=".ActivityScanner"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ActivityMenuPrincipal"
            android:exported="true" />
    </application>

</manifest>